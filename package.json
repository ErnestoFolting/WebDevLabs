    "lite-server": "^2.6.1",
    "prettier": "2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "git add "
    ],
    "*.css": [
      "npm run lintcss",
      "git add "
    ],
    "*html": [
      "npm run prettiercheck",
      "git add "
    ]
  },
  "scripts": {
    "lint": "eslint **/*.js --no-error-on-unmatched-pattern",
    "lintcss": "stylelint **/*.css",
    "prettiercheck": "prettier **/*.html --check",
    "start": "lite-server"
  },
